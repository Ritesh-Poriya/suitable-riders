    "moment": "^2.29.1",
    "mongoose": "^6.1.2",
    "nest-winston": "^1.6.2",
    "nestjs-i18n": "^8.2.2",
    "nodemailer": "^6.7.2",
    "redis": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
