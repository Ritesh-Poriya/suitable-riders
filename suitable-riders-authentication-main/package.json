  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=dev nest start",
    "start:local": "cross-env NODE_ENV=local nest start --watch",
    "start:debug": "cross-env NODE_ENV=local nest start --debug --watch",
    "start:dev": "cross-env NODE_ENV=dev node dist/main",
    "watch:dev": "cross-env NODE_ENV=dev nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=prod node dist/main",
    "start:test": "cross-env NODE_ENV=test node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "migrate:dev:status": "cross-env NODE_ENV=dev migrate-mongo status",
    "migrate:prod:status": "cross-env NODE_ENV=prod migrate-mongo status",
    "migrate:dev:up": "cross-env NODE_ENV=dev migrate-mongo up",
    "migrate:prod:up": "cross-env NODE_ENV=prod migrate-mongo up",
    "migrate:dev:down": "cross-env NODE_ENV=dev migrate-mongo down",
    "migrate:prod:down": "cross-env NODE_ENV=prod migrate-mongo down"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.4",
    "@nestjs/bull": "^0.4.2",
