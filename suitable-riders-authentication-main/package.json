    "@nestjs/mongoose": "^9.0.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/terminus": "^8.0.3",
    "@types/archiver": "^5.1.1",
    "@types/bcrypt": "^5.0.0",
    "@types/ejs": "^3.1.0",
    "@types/glob": "^7.2.0",
    "archiver": "^5.3.0",
    "bcrypt": "^5.0.1",
    "bull": "^3.29.3",
